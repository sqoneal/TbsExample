<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="top|left"
    tools:context="com.liebao.zzj.tbsexample.MainActivity">

    <FrameLayout
        android:id="@+id/mzwebframelayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusable="true">

        <!--<com.tencent.smtt.sdk.WebView
            android:id="@+id/mzTSBWebView"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:paddingLeft="5dp"
            android:paddingRight="5dp" />-->
    </FrameLayout>

    <FrameLayout
        android:id="@+id/mzvideoFrameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></FrameLayout>

    <RelativeLayout
        android:id="@+id/llayout1"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:background="@color/viewbackground"
        android:focusable="false">

        <ImageView
            android:id="@+id/mzImageView"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/refresh" />

        <EditText
            android:id="@+id/mzEditText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/mzImageView"
            android:inputType="text"
            android:lines="1"
            android:maxLines="1"
            android:singleLine="true"
            android:text="@string/index_site" />

        <ProgressBar
            android:id="@+id/mzprogressBar1"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="match_parent"
            android:layout_height="3dp"
            android:layout_alignParentBottom="true"
            android:progressDrawable="@drawable/pg"
            android:visibility="gone" />
    </RelativeLayout>

    <LinearLayout
        android:clickable="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="bottom">

        <LinearLayout
            android:id="@+id/mznewtablayout"
            android:background="@drawable/layout_border"
            android:divider="@drawable/separator"
            android:focusable="false"
            android:visibility="gone"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/mznewtabtextview"
                android:textSize="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/newtab" />
            <View
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/black"/>
            <TextView
                android:id="@+id/mzclosetextview"
                android:textSize="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/closetab" />
            <View
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/black"/>
            <TextView
                android:id="@+id/mzmanagebookmarktextview"
                android:textSize="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/managebookmark" />
            <View
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/black"/>
            <TextView
                android:id="@+id/mzmanagedownloadtextview"
                android:textSize="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/managedownload" />
            <View
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@android:color/black"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mzToollayout"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/viewbackground"
            android:focusable="false"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/mz_back_imageview"
                android:layout_width="30dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:src="@drawable/back" />

            <ImageView
                android:id="@+id/mz_forward_imageview"
                android:layout_width="30dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:src="@drawable/forward" />

            <ImageView
                android:id="@+id/mz_add_imageview"
                android:layout_width="30dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:src="@drawable/add" />

            <ImageView
                android:id="@+id/mz_bookmark_imageview"
                android:layout_width="30dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:src="@drawable/bookmark1" />

            <ImageView
                android:id="@+id/mz_share_imageview"
                android:layout_width="30dp"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:src="@drawable/share" />
        </LinearLayout>
    </LinearLayout>
</FrameLayout>
